# ================================
# Flavor Studios Example .env File
# ================================

# --- Site Base URL ---
NEXT_PUBLIC_BASE_URL=https://flavorstudios.in
BASE_URL=https://flavorstudios.in

# --- Bing Search API Key ---
BING_API_KEY=your_bing_api_key_here

# --- IndexNow API Key (for instant search indexing) ---
INDEXNOW_KEY=your_indexnow_key_here

# --- Admin API Protection ---
ADMIN_EMAIL=your_admin_email_here
ADMIN_PASSWORD=your_admin_password_here
ADMIN_SECRET=your_admin_secret_here

# --- Firebase Web App ---
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain_here
FIREBASE_PROJECT_ID=your_firebase_project_id_here
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket_here
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id_here
FIREBASE_APP_ID=your_firebase_app_id_here
FIREBASE_VAPID_KEY=your_firebase_vapid_key_here
